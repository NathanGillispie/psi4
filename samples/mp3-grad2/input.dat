#! MP3 cc-pVDZ gradient for the NO radical


molecule no {
0 2
n
o 1 1.158
}

set {
  basis cc-pvdz
  reference uhf
  guess gwh
  mp_type conv
}

if psi4.core.get_option("scf", "orbital_optimizer_package") != "INTERNAL":
    psi4.set_options({"e_convergence": 9, "d_convergence": 5e-9})

grad = gradient('mp3')


