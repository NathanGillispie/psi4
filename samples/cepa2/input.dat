#! cc-pvdz H2O Test ACPF Energy/Properties
molecule h2o {
0 1
O
H 1 1.0 
H 1 1.0 2 104.5
}

set {
  e_convergence 1e-10
  d_convergence 1e-10
  r_convergence 1e-10
  basis cc-pvdz
  freeze_core true
  dipmom true
  print 2
}

if psi4.core.get_option("scf", "orbital_optimizer_package") != "INTERNAL":
    psi4.set_options({"e_convergence": 9, "d_convergence": 5e-9})

energy('acpf')



clean()
